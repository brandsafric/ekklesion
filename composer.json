{
    "name": "ekklesion/people",
    "description": "People management for churches",
    "type": "project",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "slim/slim": "^3.11",
        "doctrine/orm": "^2.6",
        "doctrine/migrations": "^1.8",
        "psr/container": "^1.0",
        "monolog/monolog": "^1.23",
        "twig/twig": "^2.5",
        "league/tactician": "^1.0",
        "zeuxisoo/slim-whoops": "^0.6.4",
        "mnc-ddd/doctrine-collection": "^1.0",
        "ramsey/uuid": "^3.8",
        "cakephp/chronos": "^1.2",
        "league/tactician-doctrine": "^1.1",
        "league/tactician-logger": "^0.10.0",
        "beberlei/assert": "^3.1",
        "hansott/psr7-cookies": "^2.0",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
