        "symfony/dotenv": "^4.1",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matias Navarro Carter",
            "email": "mnavarrocarter@gmail.com"
        }
